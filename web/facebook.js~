console.log("Facebook image navigation");

var next = document.getElementsByClassName('photoPageNextNav')[0];
var prev = document.getElementsByClassName('photoPagePrevNav')[0];

function leftAction() {
		prev.onclick();
}
function rightAction() {
		next.onclick();
}
function downAction() {
		chrome.downloads.download("https://fbcdn-sphotos-b-a.akamaihd.net/hphotos-ak-ash4/295321_10150943141353915_1125890917_n.jpg");		
}
function upAction() {
		
}
